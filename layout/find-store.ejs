<script>
    const selectBar = document.querySelectorAll('.select-bar-link');

    selectBar.forEach(btnType => {
        btnType.addEventListener('click', () => {
            selectBar.forEach(btn => {
                btn.classList.remove('active');
            })
            btnType.classList.add('active');
        })
    })
</script>